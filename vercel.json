{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/widget",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "regions": ["iad1"]
} 